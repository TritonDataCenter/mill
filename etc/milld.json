{
    "sources": [
        {
            "service": "foo",
            "instance": "grape",
            "interval": 300000,
            "location": "/var/tmp/foo.log",
            "logType": "bunyan",
            "format": "$remote_addr - $remote_user [$time_local] $request \"$status\" $body_bytes_sent \"$http_referer\" \"$http_user_agent\" \"$http_x_forwarded_for\""
        },
        {
            "service": "bar",
            "instance": "grape",
            "interval": 300000,
            "location": "/var/tmp/bar.log",
            "logType": "bunyan",
            "format": "$remote_addr - $remote_user [$time_local] $request \"$status\" $body_bytes_sent \"$http_referer\" \"$http_user_agent\" \"$http_x_forwarded_for\""
        }
    ]
}
